{
  "name": "@apto/aptotude",
  "license": "MIT",
  "version": "1.0.80",
  "private": false,
  "description": "Apto UI for React",
  "author": "Apto",
  "homepage": "https://github.com/aptotude/aptotude",
  "bugs": {
    "url": "https://github.com/aptotude/aptotude/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aptotude/aptotude.git"
  },
  "scripts": {
    "build": "yarn build:setup; rollup -c",
    "build:setup": "./scripts/build-setup.sh",
    "dev": "yarn storybook:local",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,json,js,jsx,css}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,scss,json,js,jsx,css}\"",
    "lint": "tslint --project .",
    "release": "./scripts/release.sh",
    "storybook:local": "start-storybook -p 9001 -s ./src/assets -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "test": "jest",
    "test:ci": "yarn test --maxWorkers=2",
    "test:update": "yarn test -u",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "^10.11",
    "yarn": "^1.3.2"
  },
  "main": "aptotude.cjs.js",
  "module": "aptotude.esm.js",
  "browser": "aptotude.umd.js",
  "types": "index.d.ts",
  "dependencies": {
    "@apto/icons": "^1.0.7",
    "@reach/router": "^1.2.1",
    "classnames": "^2.2.6",
    "faker": "^4.1.0",
    "formik": "^1.4.3",
    "lodash.debounce": "^4.0.8",
    "pretty-bytes": "^5.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^8.0.4",
    "react-event-listener": "^0.6.6",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.84.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/addon-storysource": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/classnames": "^2.2.6",
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.3.9",
    "@types/lodash.debounce": "^4.0.4",
    "@types/node": "^10.12.18",
    "@types/pretty-bytes": "^5.1.0",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-event-listener": "^0.4.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-options": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.1",
    "jest": "^23.6.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.15.3",
    "react-testing-library": "^5.3.0",
    "rollup": "^0.68.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-generate-package-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-typescript2": "^0.18.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.1.1",
    "webpack": "^4.28.4"
  }
}
